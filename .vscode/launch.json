    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch and Debug Standalone Blazor WebAssembly App",
            "type": "blazorwasm",
            "request": "launch",
            "cwd": "${workspaceFolder}/OldiOS/OldiOS.Web"
        },
        {
            "name": "Debug MAUI Windows",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build-maui-windows",
            "program": "${workspaceFolder}/OldiOS/OldiOS/bin/Debug/net9.0-windows10.0.19041.0/win10-x64/OldiOS.exe",
            "args": [],
            "cwd": "${workspaceFolder}/OldiOS/OldiOS",
            "stopAtEntry": false,
            "console": "internalConsole"
        }
    ]
}